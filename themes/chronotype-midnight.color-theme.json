  // The themes syntaxes are made with a palette derived from the
  // main UI accent. What's actually hightlighted was chosen with the
  // Alabaster Theme philosophy in mind.
  //
  // I added three additional categories:
  // 1) syntax I don't want highlighted explicitly
  // 2) syntax I want to deemphasize
  // 3) idioms of a language
  //
  // I use font variations because they help me read code better,
  // but feel free to remove them if they're distracting for you.
  // =============================================================
  // 1. Backdrop: #666668
  // 2. Strings: #f98f48
  // 3. Constants: #d1ac34
  // 4. Comments: #f95b88
  // 5. Globals: #9fc249
  // 6. Idioms: #068be2
  // 7. Negations: #cfcecc
  "tokenColors": [
    {
      "name": "1. Backdrop",
      "scope": [
        "keyword",
        "const.character.escape",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#47474a"
      }
    },
    {
      "name": "2. Strings",
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#f98f48"
      }
    },
    {
      "name": "3. Constants",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "support.constant",
        "constant"
      ],
      "settings": {
        "foreground": "#d1ac34",
        "fontStyle": "bold"
      }
    },
    {
      "name": "4. Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.preprocessor",
        "comment.block.documentation",
        "comment.documentation"
      ],
      "settings": {
        "foreground": "#f95b88"
      }
    },
    {
      "name": "5. Globals",
      "scope": [
        "entity.name",
      ],
      "settings": {
        "foreground": "#9fc249"
      }
    },
    {
      "name": "6. Idioms",
      "scope": [
        "variable.language",
        "storage.modifier",
        "meta.selector"
      ],
      "settings": {
        "foreground": "#068be2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "7. Negations",
      "scope": [
        "support.type.property-name.json",
        "keyword.operator",
      ],
      "settings": {
        "foreground": "#cfcecc"
      }
    }
  ]
}
