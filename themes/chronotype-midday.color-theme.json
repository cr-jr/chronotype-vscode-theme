  // The themes syntaxes are made with a palette derived from the
  // UI. What's actually hightlighted was chosen with the Alabaster Theme
  // philosophy in mind. However...
  //
  // I added three additional categories:
  // 1) syntax I don't want highlighted explicitly
  // 2) backdrop general language keywords
  // 3) idioms of a language
  //
  // I use font variations because they help me read code better,
  // but feel free to remove them if they're distracting for you.
  // =============================================================
  // 1. Backdrop: #666668
  // 2. Strings: #97608e
  // 3. Constants: #33b18f
  // 4. Comments: #869859
  // 5. Globals: #3390be
  // 6. Idioms: #cc7c58
  // 7. Negations: #292a2e
  "tokenColors": [
    {
      "name": "1. Backdrop",
      "scope": [
        "keyword",
        "const.character.escape",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#666668"
      }
    },
    {
      "name": "2. Strings",
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#97608e"
      }
    },
    {
      "name": "3. Constants",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "support.constant",
        "constant"
      ],
      "settings": {
        "foreground": "#33b18f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "4. Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.preprocessor",
        "comment.block.documentation",
        "comment.documentation"
      ],
      "settings": {
        "foreground": "#869859"
      }
    },
    {
      "name": "5. Globals",
      "scope": [
        "entity.name",
      ],
      "settings": {
        "foreground": "#3390be"
      }
    },
    {
      "name": "6. Idioms",
      "scope": [
        "variable.language",
        "storage.modifier",
        "meta.selector"
      ],
      "settings": {
        "foreground": "#cc7c58",
        "fontStyle": "bold"
      }
    },
    {
      "name": "7. Negations",
      "scope": [
        "support.type.property-name.json",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#292a2e"
      }
    }
  ]
}
