	"tokenColors": [
		{
			"settings": {
				"foreground": "#e9e8dfff",
				"background": "#191a28ff"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
        "punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#b1a59fff"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"foreground": "#b1a59fff"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#869859ff"
			}
		},
		{
      "name": "Invalid - Deprecated",
      "scope": "invalid.deprecated",
			"settings": {
				"background": "#cc627814"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#cc627814",
				"foreground": "#974959ff"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#594a54ff"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
        "storage"
