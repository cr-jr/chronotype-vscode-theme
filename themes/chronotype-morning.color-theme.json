			"settings": {
				"foreground": "#191a28ff"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#97608eff"
			}
		},
