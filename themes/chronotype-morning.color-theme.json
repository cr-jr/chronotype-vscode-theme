	"tokenColors": [
		{
			"settings": {
				"foreground": "#e9e8dfff",
				"background": "#39313eff"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
        "punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#b1a59fff"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"foreground": "#b1a59fff"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#869859ff"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#594a54ff"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
        "storage"
			],
			"settings": {
        "foreground": "#3390beff",
        "fontStyle": "italic"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
        "foreground": "#3390beff",
        "fontStyle": "bold"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
        "foreground": "#3390beff",
        "fontStyle": "bold"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#39313eff"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#33b18fff"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#cc7c58ff"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#a48b4fff"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
        "foreground": "#39313eff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Characters",
			"scope": [
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#337abcff"
			}
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#cc6278",
        "fontStyle": "bold"
      }
    },
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#869859ff"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#b1a59fff"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
        "foreground": "#cc6278ff",
        "fontStyle": "underline"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#a48b4fff"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#594a54ff"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#39313eff"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
        "foreground": "#33b18f",
        "fontStyle": "bold"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#39313eff"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#97608eff"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
        "foreground": "#33b18fff",
        "fontStyle": "bold"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
        "support.type.property-name",

			],
			"settings": {
				"foreground": "#39313eff"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#97608e"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
    },
    {
      "name": "JS/TS(X): Object Literal Keys",
      "scope": "meta.object-literal.key",
      "settings": {
        "foreground": "#39313eff",
        "fontStyle": "bold"
      }
    },
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#3398beff"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#594a54ff"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#594a54ff"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
