				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
        "foreground": "#3390beff",
        "fontStyle": "bold"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#97608eff"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
