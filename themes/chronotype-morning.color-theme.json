			"settings": {
				"fontStyle": "bold",
				"foreground": "#33b18fff"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#cc7c58ff"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#a48b4fff"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
        "foreground": "#39313eff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Characters",
			"scope": [
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#337abcff"
			}
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#cc6278",
        "fontStyle": "bold"
      }
    },
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#869859ff"
			}
		},
		{
			"name": "Strings: Escape Sequences",
